{
  "rules": {
    "no-constant-condition": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "use-isnan": 2,
    "valid-jsdoc": [1, {
        "requireReturn": false
    }],
    "valid-typeof": 2,
    "accessor-pairs": 2,
    "block-scoped-var": 2,
    "consistent-return": 1,
    "curly": 2,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-else-return": 1,
    "no-empty-label": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-implicit-coercion": 1,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-spaces": [1, { "exceptions": { "VariableDeclarator": true, "Property": true, "ImportDeclaration": true, "AssignmentExpression": true } }],
    "no-multi-str": 1,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 1,
    "no-octal": 1,
    "no-param-reassign": 1,
    "no-proto": 1,
    "no-redeclare": 2,
    "no-return-assign": 1,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 1,
    "no-unused-expressions": [1, { "allowShortCircuit": true, "allowTernary": true }],
    "no-void": 2,
    "no-with": 2,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": [1, "inside"],
    "yoda": 1,
    "strict": [1, "global"],
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "handle-callback-err": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1,
    "consistent-this": [1, "self"],
    "eol-last": 1,
    "indent": 1,
    "key-spacing": [1, { "align": "colon" }],
    "linebreak-style": 2,
    "max-len": [1, 120, 4],
    "new-cap": 1,
    "new-parens": 1,
    "no-array-constructor": 1,
    "no-inline-comments": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": [1, {"max": 1}],
    "no-new-object": 1,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 1,
    "operator-linebreak": [1, "after"],
    "require-jsdoc": [1, {
      "require": {
        "FunctionDeclaration": true,
        "MethodDefinition": false,
        "ClassDeclaration": false
      }
    }],
    "semi-spacing": 1,
    "semi": 1,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": 1,
    "space-before-keywords": [1, "always"],
    "space-in-parens": 1,
    "jsx-quotes": [1, "prefer-double"],
    "space-infix-ops": 1,
    "space-return-throw-case": 1,
    "spaced-comment": 1,
    "arrow-spacing": 1,
    "constructor-super": 1,
    "no-class-assign": 1,
    "no-dupe-class-members": 1,
    "no-const-assign": 1,
    "no-this-before-super": 2,
    "no-var": 1,
    "object-shorthand": 1,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-template": 1
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  }
}
