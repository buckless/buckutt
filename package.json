{
  "name": "buckless-admin",
  "version": "1.0.0",
  "description": "buckless-admin",
  "author": "Buckless <contact@buckless.com> https://buckless.com",
  "contributors": [
    "Gabriel Juchault <gabriel@buckless.com>",
    "Axel Mousset <axel@buckless.com>",
    "Lucas Soulier <lucas@buckless.com>"
  ],
  "repository": "https://lab.buckless.com/core/admin.git",
  "homepage": "https://buckless.com",
  "scripts": {
    "start": "static dist --host-address 0.0.0.0 --port 8082",
    "build": "vue-cli-service build",
    "build:docker": "docker build -t lab.buckless.com:4567/core/admin .",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "@buckless/datetime-picker": "^0.2.2",
    "bcryptjs": "^2.4.3",
    "c3": "^0.6.7",
    "file-saver": "^1.3.8",
    "fuzzy": "^0.1.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.memoize": "^4.1.2",
    "lodash.pick": "^4.4.0",
    "lodash.samplesize": "^4.2.0",
    "lodash.set": "^4.3.2",
    "material-design-lite": "^1.3.0",
    "moment": "^2.22.2",
    "node-static": "^0.7.10",
    "promise-polyfill": "^8.1.0",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-mdl": "^1.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "config": true,
      "moment": true,
      "componentHandler": true,
      "MaterialTextfield": true
    }
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-mixins": {},
      "postcss-custom-properties": {},
      "postcss-simple-vars": {},
      "postcss-calc": {},
      "postcss-color-function": {},
      "postcss-custom-media": {},
      "postcss-nested": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "optionalDependencies": {
    "cypress": "3.1.0"
  }
}
