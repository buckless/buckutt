{
  "name": "buckless-client",
  "version": "1.0.0",
  "description": "buckless-client",
  "author": "Buckless <contact@buckless.com> https://buckless.com",
  "contributors": [
    "Gabriel Juchault <gabriel@buckless.com>",
    "Axel Mousset <axel@buckless.com>",
    "Lucas Soulier <lucas@buckless.com>"
  ],
  "repository": "https://lab.buckless.com/core/client.git",
  "homepage": "https://buckless.com",
  "private": true,
  "license": "MIT",
  "main": "src/main.js",
  "scripts": {
    "lint": "prettier --single-quote --print-width 100 --tab-width 4 --write \"src/**/*.{js,vue}\"",
    "buildmobile": "cd cordova && cordova prepare",
    "buildnative": "electron-rebuild .",
    "build:docker": "docker build -t lab.buckless.com:4567/core/client .",
    "build:android": "cross-env TARGET=cordova PLATFORM=android node build/build.js",
    "build:electron": "cross-env TARGET=electron node build/build.js",
    "build:browser": "cross-env TARGET=browser node build/build.js",
    "android": "cd cordova && cordova run android --emulator",
    "android:device": "cd cordova && cordova run android --device",
    "electron": "electron .",
    "browser": "static dist/browser --port 8081",
    "dev:android": "cross-env TARGET=cordova NODE_ENV=development PLATFORM=android webpack --config build/webpack.dev.conf.js --progress --hide-modules --watch",
    "dev:browser": "cross-env TARGET=browser NODE_ENV=development webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "dev:electron": "cross-env TARGET=electron NODE_ENV=development webpack-dev-server --inline --config build/webpack.dev.conf.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "prettier --single-quote --print-width 100 --tab-width 4 --write",
      "git add"
    ]
  },
  "dependencies": {
    "@buckless/offline-queue": "^1.0.3",
    "@buckless/signed-data": "^2.2.0",
    "@pokusew/pcsclite": "^0.4.18",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "circular-json": "^0.5.5",
    "dexie": "^2.0.4",
    "electron": "^2.0.5",
    "file-saver": "^1.3.8",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.0.0",
    "localforage": "^1.7.2",
    "lodash.chunk": "^4.2.0",
    "lodash.countby": "^4.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.1",
    "lodash.sumby": "^4.6.0",
    "lodash.uniqby": "^4.7.0",
    "msgpack-lite": "^0.1.26",
    "node-machine-id": "^1.1.10",
    "normalize.css": "^8.0.0",
    "postcss-mixins": "^6.2.0",
    "raven-js": "^3.26.4",
    "readline": "^1.3.0",
    "rusha": "^0.8.13",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "cordova": "^8.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron-rebuild": "^1.8.1",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "node-notifier": "^5.1.2",
    "node-static": "^0.7.10",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "ora": "^3.0.0",
    "portfinder": "^1.0.13",
