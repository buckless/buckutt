{
  "name": "buckless-server",
  "version": "1.0.1",
  "description": "buckless-server",
  "author": "Buckless <contact@buckless.com> https://buckless.com",
  "contributors": [
    "Gabriel Juchault <gabriel@buckless.com>",
    "Axel Mousset <axel@buckless.com>",
    "Lucas Soulier <lucas@buckless.com>"
  ],
  "repository": "https://lab.buckless.com/core/server.git",
  "homepage": "https://buckless.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node src/app",
    "test": "cross-env nyc --reporter=text-summary --reporter=html --reporter=lcov mocha ./test/bootstrap.js",
    "build:docker": "docker build -t lab.buckless.com:4567/core/server .",
    "watch": "nodemon src/app.js",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "seed": "cross-env NODE_ENV=development node scripts/seed",
    "lint": "prettier --single-quote --print-width 100 --tab-width 4 --write \"src/**/*.js\"",
    "sslConfig": "node scripts/sslConfig",
    "addDevice": "node scripts/addDevice",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --print-width 100 --tab-width 4",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "body-parser-with-msgpack": "^1.17.0",
    "bookshelf": "^0.12.1",
    "bookshelf-manager": "^0.3.0",
    "bookshelf-paranoia": "^0.11.0",
    "bookshelf-signals": "^0.1.2",
    "bookshelf-uuid": "^1.0.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.2",
    "cors": "^2.7.1",
    "cross-env": "^5.1.3",
    "csv": "^2.0.0",
    "dot": "^1.1.2",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "flav-payline": "^0.1.4",
    "fs-extra": "^5.0.0",
    "husky": "^1.0.0-rc.2",
    "jsonwebtoken": "^8.1.1",
    "knex": "^0.14.2",
    "leven": "^2.1.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "moment-round": "^1.0.1",
    "morgan": "^1.8.2",
    "nconf": "^0.10.0",
    "node-etupay": "ungdev/node-etupay",
    "nodemailer": "^4.4.2",
    "nyc": "^11.2.1",
    "pg": "^7.4.1",
    "qs": "^6.1.0",
    "randomstring": "^1.1.5",
    "redis": "^2.8.0",
    "rimraf": "^2.5.2",
    "socket.io": "^2.0.3",
    "transliteration": "^1.6.2",
    "util-promisifyall": "^1.0.3",
    "uuid": "^3.2.1",
    "uws": "^9.14.0",
    "winston": "^2.1.1",
    "winston-tcp-graylog": "PymZor/winston-tcp-graylog#refact-native-js"
  },
  "devDependencies": {
    "connect-sdk-nodejs": "^2.15.0",
    "coveralls": "^3.0.0",
    "inquirer": "^5.0.1",
    "lint-staged": "^7.0.5",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "prettier": "1.11.1"
  },
  "resolutions": {
    "uws": "0.14.5"
  }
}
