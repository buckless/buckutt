{
  "name": "buckless-server",
  "version": "1.0.0",
  "description": "Buckless API server",
  "authors": [
    "Gabriel Juchault <gabriel.juchault@gmail.com>",
    "Axel Mousset <axel@mousset.me>",
    "Lucas Soulier <lucas.soulier.utt@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run addTestDevice",
    "cleardb": "rimraf rethinkdb_data",
    "test": "npm run test:coverage",
    "test:nocoverage": "node scripts/test",
    "test:coverage": "cross-env NODE_ENV=test node scripts/test --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "node src/app",
    "startdb": "rethinkdb&",
    "stopdb": "killall rethinkdb -s SIGINT",
    "seed": "node scripts/dataSeeds",
    "lint": "eslint . --fix",
    "sslConfig": "node scripts/sslConfig",
    "addDevice": "node scripts/addDevice",
    "addTestDevice": "rimraf ssl/test && node scripts/addTestDevice",
    "databaseAccess": "node -r ./scripts/databaseAccess"
  },
  "dependencies": {
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.0",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.2",
    "cors": "^2.7.1",
    "cross-env": "^3.1.4",
    "express": "^4.13.4",
    "fs-extra": "^0.30.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.11.1",
    "mkdirp": "^0.5.1",
    "morgan": "1.7.0",
    "nconf": "^0.8.4",
    "qs": "^6.1.0",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.0",
    "sse": "github:gjuchault/sse.js",
    "sync-exec": "^0.6.2",
    "thinky": "^2.2.4",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "elegant-status": "^1.1.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eventsource": "github:gjuchault/eventsource-node",
    "inquirer": "github:SBoudrias/Inquirer.js",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "unirest": "github:mashape/unirest-nodejs"
  }
}
