{
   "rights": {
      "all": ["admin"],
      "seller": {
          "canSell": true,
          "canReload": false,
          "canAssign": false,
          "canControl": false,
          "read": [
              "/services/items",
              "/services/pendingCardUpdate",
              "alert"
          ],
          "write": [
              "/services/basket",
              "/services/catering",
              "/services/cancelTransaction"
          ]
      },
      "reloader": {
          "canSell": false,
          "canReload": true,
          "canAssign": false,
          "canControl": false,
          "read": [
              "/services/items",
              "/services/pendingCardUpdate",
              "alert"
          ],
          "write": [
              "/services/basket",
              "/services/cancelTransaction"
          ]
      },
      "assigner": {
          "canSell": false,
          "canReload": false,
          "canAssign": true,
          "canControl": false,
          "read": [
              "/users",
              "/services/items",
              "/services/assigner",
              "/services/pendingCardUpdate",
              "alert"
          ],
          "write": [
              "/meansoflogin",
              "/services/assigner/groups",
              "/services/assigner/anon",
              "/services/basket"
          ]
      },
      "controller": {
          "canSell": false,
          "canReload": false,
          "canAssign": false,
          "canControl": true,
          "read": [
              "/services/items",
              "/services/controller",
              "alert"
          ],
          "write": [
              "/services/controller"
          ]
      },
      "openUrls": [
          "/",
          "/services/login",
          "/services/manager/askpin",
          "/services/manager/generatepin",
          "/services/manager/register",
          "/services/deviceEssentials",
          "/provider/callback",
          "/services/assigner"
      ],
      "loggedUrls": [
          "/services/manager/changepin",
          "/services/manager/history",
          "/services/manager/searchuser",
          "/services/manager/transfer",
          "/services/manager/reload",
          "/services/manager/giftReloads",
          "/services/manager/switchuser",
          "userCredit"
      ],
      "pinLoggingAllowed": ["seller", "reloader", "assigner", "controller"]
   }
}
