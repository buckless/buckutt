{
   "rights": {
      "all": ["admin"],
      "seller": {
          "canSell": true,
          "canReload": false,
          "canAssign": false,
          "read": [
              "/events",
              "/meansoflogin",
              "/meansofpayment",
              "/devices",
              "/devices/search",
              "/services/items",
              "/services/pendingCardUpdate",
              "alert"
          ],
          "write": [
              "/services/basket",
              "/services/cancelTransaction"
          ]
      },
      "reloader": {
          "canSell": false,
          "canReload": true,
          "canAssign": false,
          "read": [
              "/events",
              "/devices",
              "/meansoflogin",
              "/meansofpayment",
              "/meansofpayment/search",
              "/services/items",
              "/services/pendingCardUpdate",
              "alert"
          ],
          "write": [
              "/services/basket"
          ]
      },
      "assigner": {
          "canSell": false,
          "canReload": false,
          "canAssign": true,
          "read": [
              "/users",
              "/events",
              "/groups",
              "/meansoflogin",
              "/services/assigner",
              "/services/pendingCardUpdate"
          ],
          "write": [
              "/users",
              "/meansoflogin",
              "/services/assigner/groups"
          ]
      },
      "controller": {
          "canSell": false,
          "canReload": false,
          "canAssign": false,
          "canControl": true,
          "read": [
              "/users",
              "/groups",
              "/events",
              "/services/controller"
          ],
          "write": [
              "/accesses",
              "/services/controller"
          ]
      },
      "openUrls": [
          "/",
          "/services/login",
          "/services/manager/askpin",
          "/services/manager/generatepin",
          "/services/deviceEssentials",
          "/provider/callback",
          "/services/assigner"
      ],
      "loggedUrls": [
          "/services/manager/changepin",
          "/services/manager/history",
          "/services/manager/searchuser",
          "/services/manager/transfer",
          "/services/manager/reload",
          "userCredit"
      ],
      "pinLoggingAllowed": ["seller", "reloader", "assigner", "controller"]
   }
}
