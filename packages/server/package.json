  "scripts": {
    "start": "cross-env TS_NODE_TRANSPILE_ONLY=true ts-node app",
    "start:pm2": "cross-env TS_NODE_TRANSPILE_ONLY=true pm2-runtime start ecosystem.config.js --env production",
    "pretest": "cross-env NODE_ENV=test node test/utils/prepareDatabase",
    "test": "cross-env NODE_ENV=test nyc ava **/*.test.js --verbose -s",
    "dev": "cross-env NODE_ENV=development ts-node-dev app",
    "dev:pm2": "cross-env NODE_ENV=development pm2 start ecosystem.config.js --no-daemon",
    "lint": "prettier --write \"{app,migrations,scripts,seeds,test}/**/*.{ts,js}\"",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@types/node": "^12.0.2",
    "@ung/node-etupay": "ungdev/node-etupay",
