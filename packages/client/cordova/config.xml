<?xml version='1.0' encoding='utf-8'?>
<widget id="com.buckless.multiclient" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Buckless Cashless</name>
    <description>
        Application op√©rateur Cashless
    </description>
    <icon src="../static/icon.png" />
    <author email="contact@buckless.com" href="http://buckless.com">
        Studio Async
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="*" />
    <preference name="OverrideUserAgent" value="c" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="Fullscreen" value="true" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="Fullscreen" value="true" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-keyboard" spec="^1.2.0" />
    <plugin name="cordova-sqlite-storage" spec="^2.2.0" />
    <plugin name="cordova-plugin-x-socialsharing" spec="^5.4.0" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-android-support-gradle-release" spec="^1.2.0">
        <variable name="ANDROID_SUPPORT_VERSION" value="25.+" />
    </plugin>
    <plugin name="phonegap-plugin-media-stream" spec="^1.2.1" />
    <plugin name="cordova-plugin-mifare-ultralight" spec="https://github.com/lonk/cordova-plugin-mifare-ultralight#fix-pin-length" />
    <engine name="android" spec="^6.4.0" />
</widget>
