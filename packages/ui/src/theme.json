{
    "radius": "4px",
    "transition": {
        "fast-in": ".1s",
        "fast-out": ".075s",
        "medium-in": ".15s",
        "medium-out": ".12s",
        "long-in": ".25s",
        "long-out": ".2s",
        "easing": "cubic-bezier(0.4, 0.0, 0.2, 1)"
    },
    "colors": {
        "foreground": {
            "light": {
                "300": "rgba(255,255,255,.87)",
                "200": "rgba(255,255,255,.71)",
                "100": "rgba(255,255,255,.50)"
            },
            "dark": {
                "300": "rgba(0,0,0,.87)",
                "200": "rgba(0,0,0,.71)",
                "100": "rgba(0,0,0,.50)"
            }
        },
        "black": "#0A0D15",
        "grey": {
            "600": "#e3e3e4",
            "500": "#e6e6e7",
            "400": "#eaeaeb",
            "300": "#ececed",
            "200": "#f2f2f3",
            "100": "#fcfcfd",
            "50": "#fff"
        },
        "primary": {
            "600": "#1c233b",
            "500": "#384675",
            "400": "#5469af",
            "300": "#708cea",
            "200": "#9cadf0",
            "100": "#c4cdf6",
            "50": "#e8ebfb"
        },
        "accent": {
            "900": "#007733",
            "800": "#009849",
            "700": "#00aa55",
            "600": "#00bd63",
            "500": "#00ce6e",
            "400": "#00d783",
            "300": "#47e09b",
            "200": "#8ae9b9",
            "100": "#bbf1d4",
            "50": "#e3faee"
        },
        "error": {
            "200": "#ff8f7c",
            "300": "#f75d4f"
        },
        "success": {
            "200": "#8dff9b",
            "300": "#54e26b"
        },
        "warning": {
            "200": "#ffd95f",
            "300": "#ffa72b"
        }
    },

    "typography": {
        "family": "'Open-Sans', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol'",
        "h1": {
            "size": "40px",
            "spacing": "0.5px",
            "weight": "500"
        },
        "h2": {
            "size": "32px",
            "spacing": "0",
            "weight": "500"
        },
        "h3": {
            "size": "28px",
            "spacing": "0",
            "weight": "500"
        },
        "h4": {
            "size": "24px",
            "spacing": "0",
            "weight": "500"
        },
        "h5": {
            "size": "20px",
            "spacing": "0",
            "weight": "500"
        },
        "h6": {
            "size": "16px",
            "spacing": "0",
            "weight": "500"
        },
        "body-1": {
            "size": "16px",
            "spacing": "0",
            "weight": "normal"
        },
        "body-2": {
            "size": "14px",
            "spacing": "0.25px",
            "weight": "normal"
        },
        "button": {
            "size": "14px",
            "spacing": "1.25px",
            "weight": "500"
        }
    },

    "elevations": {
        "1dp": {
            "far": "0 1px 1px 0 rgba(0,0,0,.14)",
            "ambient": "0 2px 1px -1px rgba(0,0,0,.12)",
            "close": "0 1px 3px 0 rgba(0,0,0,.20)"
        },
        "2dp": {
            "far": "0 2px 2px 0 rgba(0,0,0,.14)",
            "ambient": "0 3px 1px -2px rgba(0,0,0,.12)",
            "close": "0 1px 5px 0 rgba(0,0,0,.20)"
        },
        "3dp": {
            "far": "0 3px 4px 0 rgba(0,0,0,.14)",
            "ambient": "0 3px 3px -2px rgba(0,0,0,.12)",
            "close": "0 1px 8px 0 rgba(0,0,0,.20)"
        },
        "4dp": {
            "far": "0 4px 5px 0 rgba(0,0,0,.14)",
            "ambient": "0 1px 10px 0 rgba(0,0,0,.12)",
            "close": "0 2px 4px -1px rgba(0,0,0,.20)"
        },
        "6dp": {
            "far": "0 6px 10px 0 rgba(0,0,0,.14)",
            "ambient": "0 1px 18px 0 rgba(0,0,0,.12)",
            "close": "0 3px 5px -1px rgba(0,0,0,.20)"
        },
        "8dp": {
            "far": "0 8px 10px 1px rgba(0,0,0,.14)",
            "ambient": "0 3px 14px 2px rgba(0,0,0,.12)",
            "close": "0 5px 5px -3px rgba(0,0,0,.20)"
        },
        "9dp": {
            "far": "0 9px 12px 1px rgba(0,0,0,.14)",
            "ambient": "0 3px 16px 2px rgba(0,0,0,.12)",
            "close": "0 5px 6px -3px rgba(0,0,0,.20)"
        },
        "12dp": {
            "far": "0 12px 17px 2px rgba(0,0,0,.14)",
            "ambient": "0 5px 22px 4px rgba(0,0,0,.12)",
            "close": "0 7px 8px -4px rgba(0,0,0,.20)"
        },
        "16dp": {
            "far": "0 16px 24px 2px rgba(0,0,0,.14)",
            "ambient": "0 6px 30px 5px rgba(0,0,0,.12)",
            "close": "0 8px 10px -5px rgba(0,0,0,.20)"
        },
        "24dp": {
            "far": "0 24px 38px 3px rgba(0,0,0,.14)",
            "ambient": "0 9px 46px 8px rgba(0,0,0,.12)",
            "close": "0 11px 15px -7px rgba(0,0,0,.20)"
        }
    }
}
